generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user_whatsapp {
  whatsID  BigInt   @id @default(autoincrement())
  msg_id   String
  in_out   String
  sender   String?
  receiver String?
  message  String?
  edate    DateTime @default(now())
}
